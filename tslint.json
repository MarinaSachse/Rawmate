{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "no-empty-interface": false,
    "ordered-imports": false,
    "no-console": false,
    "interface-over-type-literal": false,
    "jsx-no-lambda": false,
    "arrow-parens": [true, "ban-single-arg-parens"]
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
